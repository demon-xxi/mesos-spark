{
  "id": "spark-1-4-1",
  "cpus": 1,
  "mem": 1024,
  "instances": 1,
  "ports": [
    10000,
    10001
  ],
  "env": {
    "SPARK_USER": "root",
    "APPLICATION_WEB_PROXY_BASE": "/service/spark"
  },
  "cmd": "mv /mnt/mesos/sandbox/log4j.properties conf/log4j.properties && ./bin/spark-class org.apache.spark.deploy.mesos.MesosClusterDispatcher --port $PORT0 --webui-port $PORT1 --master mesos://zk://172.31.52.231:2181/mesos --zk 172.31.52.231:2181 --host $HOST --name spark-1-4-1",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "mesosphere/spark:1.4.1-hdfs",
      "network": "HOST",
      "privileged": false,
      "parameters": [],
      "forcePullImage": false
    }
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/",
      "gracePeriodSeconds": 3,
      "intervalSeconds": 10,
      "portIndex": 1,
      "timeoutSeconds": 10,
      "maxConsecutiveFailures": 3
    }
  ],
  "uris": [
    "http://downloads.mesosphere.com.s3.amazonaws.com/assets/spark/log4j.properties"
  ]
}